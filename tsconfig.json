{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "types/context.d.ts",
    "types/formfield.d.ts",
    "types/global.d.ts",
    "types/icons.d.ts",
    "types/trending.d.ts",
    "expo-env.d.ts"
  ]
}
